{
  "name": "cead-consent",
  "version": "0.1.0",
  "description": "A simple, extremely lightweight cookie and tracking consent manager",
  "main": "dist/module.js",
  "browser": "dist/browser.js",
  "keywords": ["cookie", "consent", "gdpr", "compliance", "ccpa", "privacy"],
  "author": "Alistair Shepherd <alistair@accudio.com>",
  "license": "MPL-2.0",
  "repository": "github:Accudio/cead-consent",
  "bugs": "https://github.com/Accudio/cead-consent/issues",
  "homepage": "https://github.com/Accudio/cead-consent#readme",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "run-p compile:*",
    "compile:js": "cross-env NODE_ENV=development gulp js",
    "compile:css": "cross-env NODE_ENV=development gulp css",
    "build": "run-p build:*",
    "build:js": "cross-env NODE_ENV=production gulp js",
    "build:css": "cross-env NODE_ENV=production gulp css",
    "watch": "cross-env NODE_ENV=development gulp watch",
    "dev": "run-s clean compile watch"
  },
  "dependencies": {
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.19",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-esbuild": "^0.8.5",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "rimraf": "^3.0.2",
    "sass": "^1.37.5"
  }
}
